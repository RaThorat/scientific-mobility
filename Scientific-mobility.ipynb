   "source": [
    "fg0=folium.FeatureGroup(name='Host universities cluster', show=False )\n",
    "mapa.add_child(fg0)\n",
    "fg1=folium.FeatureGroup(name='Guest universities cluster' )\n",
    "mapa.add_child(fg1)"
   ]
  },
